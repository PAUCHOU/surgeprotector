<%= form_for [@user, @price_query] do |f| %>

  <div class="form-group">
    <%= f.label :start_address %>
    <%= f.text_field :start_address, class: "form-control", autofocus: true %>
  </div>

  <div class="form-group">
    <%= f.label :nickname %>
    <%= f.text_field :nickname, class: "form-control", autofocus: true %>
  </div>

  <div class="form-group">
   <%= f.submit "Save Place", class: "form-control btn btn-primary" %>
  </div>

<% end %>